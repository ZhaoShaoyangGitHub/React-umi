{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.4.12",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.17",
    "lint-staged": "^10.2.13",
    "mockjs": "^1.1.0",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "redux-logger": "^3.0.6",
    "umi": "^3.2.17",
    "umi-request": "^1.3.5",
    "yorkie": "^2.0.0"
  }
}
