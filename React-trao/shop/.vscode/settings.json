{
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.words": [
    "Bgword",
    "Infobox",
    "Schedul",
    "Serveies",
    "Unmount",
    "Wxml",
    "appid",
    "miniprogram",
    "preload",
    "wechat"
  ]
}
